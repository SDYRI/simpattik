@model TasikmalayaKota.Simpatik.Web.Services.mOpd.Models.mOpdModel
@{
    ViewData["Title"] = ViewBag.Title;
}

<div>
    <div class="form-row">
        <div class="form-group col-md-4">
            <div class="e-float-input e-control-wrapper">
                <ejs-textbox id="KodeSubOpd" cssClass="e-input" placeholder="Kode Urusan" floatLabelType="Always" ejs-for="@Model.KodeSubOpd"></ejs-textbox>
            </div>
        </div>
        <div class="form-group col-md-8">
            <div class="e-float-input e-control-wrapper">
                <ejs-textbox id="NamaSubOpd" cssClass="e-input" placeholder="Nama Urusan" floatLabelType="Always" ejs-for="@Model.NamaSubOpd"></ejs-textbox>
            </div>
        </div>
    </div>
</div>

<ejs-scripts></ejs-scripts>
