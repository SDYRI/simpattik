<ejs-grid id="Grid" height="490" allowPaging="true" allowResizing="true" allowSorting="true" allowReordering="true" allowGrouping="false"
          allowFiltering="true" allowSelection="true">
    <e-data-manager url="/Paket/PaketReview" adaptor="UrlAdaptor"></e-data-manager>
    <e-grid-pagesettings pageSize="12" />
    <e-grid-filterSettings type="Menu"></e-grid-filterSettings>
    <e-grid-editSettings allowAdding="true" allowEditing="true" mode="Dialog"></e-grid-editSettings>
    <e-grid-selectionsettings type="Single"></e-grid-selectionsettings>
    <e-grid-columns>
        <e-grid-column field="idpaket" headerText="idpaket" isPrimaryKey="true" isIdentity="true" visible="false"></e-grid-column>
        <e-grid-column field="nmpaket" headerText="Nama Paket"></e-grid-column>
        <e-grid-column field="namaopd" headerText="OPD" width="200"></e-grid-column>
        <e-grid-column field="pagustlh" headerText="Pagu" width="200"></e-grid-column>
        <e-grid-column field="mtdpemilihanstblm" headerText="Metode PBJ Sebelum" width="200"></e-grid-column>
        <e-grid-column field="mtdpemilihanstlh" headerText="Metode PBJ Sesudah" width="200"></e-grid-column>
        <e-grid-column field="keteranganmetode" headerText="Keterangan" width="200"></e-grid-column>
    </e-grid-columns>
</ejs-grid>

