@model TasikmalayaKota.Simpatik.Web.Services.mKodeRekening.Models.mKodeRekeningModel
@{
    ViewData["Title"] = ViewBag.Title;
}

<div>
    <div class="form-row">
        <div class="form-group col-md-4">
            <div class="e-float-input e-control-wrapper">
                <ejs-textbox id="KodeSubRincian" cssClass="e-input" placeholder="Kode Akun" floatLabelType="Always" ejs-for="@Model.KodeSubRincian"></ejs-textbox>
            </div>
        </div>
        <div class="form-group col-md-8">
            <div class="e-float-input e-control-wrapper">
                <ejs-textbox id="NamaSubRincian" cssClass="e-input" placeholder="Nama Akun" floatLabelType="Always" ejs-for="@Model.NamaSubRincian"></ejs-textbox>
            </div>
        </div>
    </div>
</div>

<ejs-scripts></ejs-scripts>
